<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.service.llamaServers</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>/Users/service/github/autosage/start_llama_servers.sh</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/service/github/autosage</string>

  <key>RunAtLoad</key>
  <true/>

  <key>KeepAlive</key>
  <true/>

  <key>StandardOutPath</key>
  <string>/tmp/llamaServers.log</string>

  <key>StandardErrorPath</key>
  <string>/tmp/llamaServers-error.log</string>
</dict>
</plist>

